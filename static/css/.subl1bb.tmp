body
{
	background: url(../img/base/fon.png) repeat;
	font-family: Impact;
	-webkit-appearance: none;
}
h1
{
	color: #406469;
	font-size: 72px;
	font-variant: small-caps;
	text-align: center;
	margin: 4px auto;
}
h2
{
	color: white;
	font-size: 50px;
	font-weight: normal!important;
	text-align: center;
	font-variant: small-caps;
	margin-top: 0 !important;
	margin-bottom: 15px;
}
.container {
	width: 960px !important;
	max-width: 960px !important;
	min-width: 960px !important;
	margin: 0 auto !important;
	padding: 0 !important;
}
.row, .col-xs-1, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, 
.col-xs-8, .col-xs-9, .col-xs-10, .col-xs-11, .col-xs-12
{
	
	margin-left: 0 !important;
	margin-right: 0 !important;
	padding: 0 !important;
}

.top
{
	
	height: 363px;
	background: url(../img/base/head.png) no-repeat;
	

}
.top a
{
	position: absolute;
	top: 171px;
	left: 323px;
}
.top a:last-child
{
	position: absolute;
	top: 275px;
	left: 300px;
}
.left
{
	float: left;
}
.right, .left
{
	margin-top:  22px!important;
	margin-bottom:  19px!important;
}
.right img, .right-words img
{
	float: right !important;
}
.left-words, .right-words
{
	margin-bottom:  19px;
}
.brown
{
	color: #60381a;
	

}
.call-block
{
	height: 107px;
	background: url(../img/base/block-bg.png) repeat;
}
.call-block ul
{
	margin: 15px 0;
}
.call-block ul li
{
	display: inline-block;
}
.call-block ul li:last-child
{
	margin-left: 300px;
}