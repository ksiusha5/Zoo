body
{
	background: url(../img/base/fon.png) repeat;
	font-family: Lucida Sans Unicode;
}
h1
{
	color: #406469;
	font-size: 72px;
	font-variant: small-caps;
	text-align: center;
	margin: 4px auto;
}
h2
{
	color: white;
	font-size: 50px;
	left: 200px;
	text-align: center;
	font-variant: small-caps;
	margin-top: 0 !important;
	margin-bottom: 15px;
}
.timer
{
	position: absolute !important;
	top:0;
	left: 265px;
	top: 85px;
}
.container {
	width: 960px !important;
	max-width: 960px !important;
	min-width: 960px !important;
	margin: 0 auto !important;
	padding: 0 !important;
}
.row, .col-xs-1, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, 
.col-xs-8, .col-xs-9, .col-xs-10, .col-xs-11, .col-xs-12
{
	
	margin-left: 0 !important;
	margin-right: 0 !important;
	padding: 0 !important;
}

.top
{
	
	height: 363px;
	background: url(../img/base/head.png) no-repeat;
	

}
.top a
{
	position: absolute;
	top: 171px;
	left: 323px;
}
.top a:last-child
{
	position: absolute;
	top: 275px;
	left: 300px;
}

.animal-left
{
	position: absolute;
	top: 18px;
	left: 133px;
}
.animal-right
{
	position: absolute;
	top: 18px;
	left: 730px;
	-moz-transform: scaleX(-1);
	-o-transform: scaleX(-1);
	-webkit-transform: scaleX(-1);
	transform: scaleX(-1);
	filter: FlipH;
	-ms-filter: "FlipH";
}

.left
{
	float: left;
}
.right, .left
{
	margin-top:  27px!important;
	margin-bottom:  25px!important;
}
.left img
{
	margin-left: -5px !important;
}

.right img
{
	float: right !important;
	margin-right: -5px;
}
.left-words, .right-words
{
	margin-bottom:  -20px;
}
.brown
{
	color: #60381a;
	font-family: impact;
}
.super
{
	color:#406469;
	font-family: impact;
}
.call-block
{
	height: 107px;
	background: url(../img/base/block-bg.png) repeat;
}
.call-block ul
{
	margin: 15px 0;
}
.call-block ul li
{
	display: inline-block;
}
.call-block ul li:last-child
{
	position: absolute;
	left: 600px;
	top: 20px;
}
.call-block ul li:last-child a
{
	width: 286px !important;
	height: 61px !important;
	text-decoration: none !important;
	font-family: Myriad Pro !important;
	font-style: italic;
	color:  #ffffff !important;
	font-size: 20px;
}
.call-block ul li:last-child p
{
	z-index: 100;
	position: absolute;
	top: 15px;
	left: 50px;
}
.my-form
{
	width: 800px;
	height: 500px;
}